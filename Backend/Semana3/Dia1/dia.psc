Algoritmo sin_titulo
	definir dia Como Entero;
	
	Escribir "ingresa el numero de dia"
	leer dia;
	Segun dia Hacer
		1:
			Escribir "lunes"
		2:
			escribir "martes"
		3:
			escribir "miercoles"
		4:
			Escribir "jueves"
		5:
			escribir "viernes"
		6:
			escribir "sabado"
		7:
			Escribir "domingo"
		De Otro Modo:
			Escribir "invalido"
	Fin Segun
FinAlgoritmo
